$busybox echo "PROFILE SUCESSFULLY APPLIED. CLOSE THIS WINDOW TAB OR THIS MESSAGE WILL KEEP HERE FOREVER."
$busybox echo "PROFILE SUCESSFULLY APPLIED. CLOSE THIS WINDOW TAB OR THIS MESSAGE WILL KEEP HERE FOREVER."
	echo ""

# ELECTROACTIVE 

 elif [ $CUR_GOVERNOR = "electroactive" ]; then
 for i in /sys/devices/system/cpu/cpufreq/electroactive; do
