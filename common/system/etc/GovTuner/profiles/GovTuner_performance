		$busybox echo "67 $MIN_CPU_FREQ:57 600000:31 800000:43 1100000:55 1400000:69 1700000:82 1900000:90 2000000:95" > "$i/target_loads";
	fi
	if [ -e "$i/timer_rate" ]; then
		$busybox echo "90000" > "$i/timer_rate";
	fi
	if [ -e "$i/timer_slack" ]; then
		$busybox echo "-1" > "$i/timer_slack";
	fi
	if [ -e "$i/two_phase_freq" ]; then
		$busybox echo "1400000,1400000,1400000,1400000" > "$i/two_phase_freq";
	fi
