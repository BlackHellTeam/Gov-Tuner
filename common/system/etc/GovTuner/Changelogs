busybox="/system/etc/GovTuner/busybox"
ver="3.5 BETA 1"
divider="==============================================="

$busybox echo "CHANGELOG FOR $ver"
$busybox echo "-------------------"
$busybox echo ""
$busybox echo "Updated balanced interactive tunables"
$busybox echo "Updated battery interactive tunables"
$busybox echo "Updated performance interactive tunables"
$busybox echo "Fixed blu_active input_boost_freq parameter for battery profile"
$busybox echo "Testing build for LSpeed"
$busybox echo ""
$busybox echo -n "b. Back to menu: "
read -r character
case $character in
b|B)$busybox clear;